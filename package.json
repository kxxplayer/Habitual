{
  "name": "habitual",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:clean": "npm run clean && npm run dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:capacitor": "next build --config=next.config.capacitor.mjs",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "clean": "npx rimraf .next && npx rimraf out && npx rimraf node_modules/.cache",
    "genkit:dev": "genkit start",
    "genkit:watch": "genkit start --watch",
    "genkit:stop": "pkill -f genkit || true",
    "capacitor:sync": "npx cap sync",
    "capacitor:build": "npm run build:capacitor && npx cap sync && npx cap build android",
    "capacitor:run": "npm run build:capacitor && npx cap sync && npx cap run android",
    "capacitor:dev": "npm run build:capacitor && npx cap sync && npx cap run android --livereload",
    "android:build": "npx cap build android",
    "android:run": "npx cap run android",
    "android:open": "npx cap open android",
    "deploy:vercel": "npm run build && vercel deploy --prod",
    "test:build": "npm run build && npm run start"
  },
  "dependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/core": "^7.3.0",
    "@capacitor/local-notifications": "^7.0.1",
    "@genkit-ai/core": "^1.12.0",
    "@genkit-ai/firebase": "^1.12.0",
    "@genkit-ai/googleai": "^1.12.0",
    "@genkit-ai/next": "^1.12.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cordova-plugin-camera": "^8.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "firebase": "^11.7.0",
    "framer-motion": "^12.17.3",
    "lucide-react": "^0.475.0",
    "next": "^15.3.2",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.3.0",
    "@next/bundle-analyzer": "^15.3.3",
    "@types/cors": "^2.8.19",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "9.28.0",
    "eslint-config-next": "15.3.3",
    "genkit": "^1.12.0",
    "genkit-cli": "^1.8.0",
    "postcss": "^8",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
